<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="350.0" style="-fx-background-color: #001835;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlers.logincontrol">
   <children>
      <Label minWidth="-Infinity" prefHeight="31.0" prefWidth="65.0" text="login" textFill="#fcf2f2">
         <font>
            <Font name="Arial" size="28.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" right="10.0" />
         </VBox.margin>
      </Label>
      <Label text="username" textFill="#fff6f6">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" right="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tex" focusTraversable="false" prefHeight="31.0" prefWidth="280.0" promptText="username">
         <VBox.margin>
            <Insets left="80.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="TOP_LEFT" text="password" textFill="#e8e3e3">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" right="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="pw_field" focusTraversable="false" prefHeight="31.0" prefWidth="171.0" promptText="password">
         <VBox.margin>
            <Insets left="80.0" right="100.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="btn_log" focusTraversable="false" mnemonicParsing="false" onAction="#openDashboard" prefHeight="31.0" prefWidth="104.0" style="-fx-background-color: #87cefa;" text="login">
         <VBox.margin>
            <Insets right="20.0" top="40.0" />
         </VBox.margin></Button>
   </children>
</VBox>
